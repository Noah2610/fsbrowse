    "scripts": {
        "dev": "next --port 8080",
        "start": "next start",
        "build": "next build"
    },
    "dependencies": {
        "@chakra-ui/core": "^0.6.1",
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.27",
        "emotion-theming": "^10.0.27",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "swr": "^0.1.18"
    },
    "devDependencies": {
        "next": "^9.2.2",
